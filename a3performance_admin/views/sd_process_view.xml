<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_sd_process_form" model="ir.ui.view">
            <field name="name">a3performance.eval.sd.process.form</field>
            <field name="model">a3performance.eval.process</field>
            <field name="inherit_id" ref="view_process_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='sd']" position="inside">                    
                    <notebook>
                        <page string="Goals &#38; KPIs">
                            <field name="sd_goal_progress" widget="progressbar"/>
                        </page>
                        <page string="Committees">
                        </page>
                        <page string="Service">
                        </page>
                        <page string="Professional Development">
                        </page>                        
                        <page string="Professional Experience">
                        </page>
                        <page string="Summary">
                        </page>                      
                    </notebook>
                </xpath>
            </field>
        </record>
    </data>
</odoo>