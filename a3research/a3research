oc -m n:Research i:a3research_root
oc -n a3research.publisher -v Publisher -m n:Publishers p:a3research_root -g base.group_user:1000 a3.group_faculty:1111

oc -n a3research.journal -v Journal -m n:Journals p:a3research_root -g base.group_user:1000 a3.group_faculty:1110

oc -n a3research.article -v Article -m n:Articles p:a3research_root -g base.group_user:1000 a3.group_faculty:1111
