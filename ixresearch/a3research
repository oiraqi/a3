oc -n ixresearch.book -v Book -m n:Books p:ixresearch_root -g base.group_user:1000 ix.group_faculty:1111

oc -n ixresearch.presentation -v Presentation -m n:Presentations p:ixresearch_root -g base.group_user:1000 ix.group_faculty:1111

